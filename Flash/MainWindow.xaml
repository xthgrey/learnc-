<Window x:Class="Flash.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:host="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
        xmlns:forms="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        xmlns:local="clr-namespace:Flash"
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="200" WindowStyle="None" MouseLeftButtonDown="Window_MouseLeftButtonDown" ResizeMode = "NoResize">
    <Grid>
        <host:WindowsFormsHost x:Name="host">
            <forms:WebBrowser x:Name="browser"></forms:WebBrowser>
        </host:WindowsFormsHost>
        <Frame Content="Frame" HorizontalAlignment="Left" Height="100" Margin="507,-79,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Window>
